[user]
	email = eloy.coto@gmail.com
	name = Eloy Coto

[alias]
    cm = commit -am
    st = status
    co = checkout
    amend = commit --amend

    branches = branch -a
    tags = tag
    stashes = stash list

    unstage = reset -q HEAD --
    discard = checkout --
    uncommit = reset --mixed HEAD~

[push]
	default = matching
